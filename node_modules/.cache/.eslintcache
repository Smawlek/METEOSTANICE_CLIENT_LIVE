, 'ST', 'ČT', 'PÁ', 'SO', 'NE'];\r\nconst curr = new Date;\r\n\r\nconst ClassroomTimetables = (props) => {\r\n    const navigate = useNavigate();\r\n\r\n    const [date, setDate] = useState(moment().year() + '-W' + (moment().week() < 10 ? '0' + moment().week() : moment().week()));\r\n    const [weekDates, setWeekDates] = useState([]);\r\n\r\n    useEffect(() => {\r\n        let temp = Number((date.split('W'))[1]);\r\n        let weeks = [\r\n            moment().week(temp).startOf('week').add(1, 'days').format('YYYY-MM-DD'),\r\n            moment().week(temp).startOf('week').add(2, 'days').format('YYYY-MM-DD'),\r\n            moment().week(temp).startOf('week').add(3, 'days').format('YYYY-MM-DD'),\r\n            moment().week(temp).startOf('week').add(4, 'days').format('YYYY-MM-DD'),\r\n            moment().week(temp).startOf('week').add(5, 'days').format('YYYY-MM-DD'),\r\n            moment().week(temp).startOf('week').add(6, 'days').format('YYYY-MM-DD'),\r\n            moment().week(temp).startOf('week').add(7, 'days').format('YYYY-MM-DD')\r\n        ];\r\n        setWeekDates(weeks);\r\n    }, [date]);\r\n\r\n    return (\r\n        <>\r\n            <div>\r\n                <h3> <b> Rozvrh učeben: {moment(weekDates[0]).format('DD.MM.YYYY')} - {moment(weekDates[6]).format('DD.MM.YYYY')} </b> </h3>\r\n            </div>\r\n            <div className='new-line'></div>\r\n\r\n            <div className=\"d-flex w-100 justify-content-between\">\r\n                <TextField\r\n                    label=\"Datum\"\r\n                    type=\"week\"\r\n                    value={date}\r\n                    onChange={(e) => { setDate(e.target.value); }}\r\n                    sx={{ width: 220 }}\r\n                    InputLabelProps={{\r\n                        shrink: true,\r\n                    }}\r\n                />\r\n                <small>\r\n                    <Tooltip title=\"Vytvořit záznam učebně\">\r\n                        <IconButton onClick={() => { navigate('/create-classroom-record') }}>\r\n                            <GoPlus />\r\n                        </IconButton>\r\n                    </Tooltip>\r\n                </small>\r\n            </div>\r\n            <div className='line-divider'></div>\r\n            {weekDates.map((val, i) => {\r\n                return (\r\n                    <>\r\n                        <ClassroomTimetablesRow key={'ClassroomTimetablesRow' + i} day={days[i]} date={val} />\r\n                        <div className='new-line'></div>\r\n                    </>\r\n                )\r\n            })}\r\n        </>\r\n    )\r\n};\r\n\r\nconst ClassroomTimetablesRow = ({ day, date }) => {\r\n    const [data, setData] = useState([]);\r\n    const [show, setShow] = useState(false);\r\n    let index = 0;\r\n\r\n    useEffect(() => {\r\n        getData();\r\n    }, []);\r\n\r\n    useEffect(() => {\r\n        getData();\r\n    }, [date])\r\n\r\n    async function getData() {\r\n        setData((await _getClassroomRecordByDate({ date: date })).data)\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <div className='row'>\r\n                <div className='col-sm-12 col-md-2 col-lg-2 classroomTimetablesRow-main classroomTimetablesRow-cell'>\r\n                    <h5> <b> {day} </b> </h5>\r\n                    <p> {moment(date).format('DD.MM.YYYY')} </p>\r\n                </div>\r\n                {data.map((val, i) => {\r\n                    return (\r\n                        <>\r\n                            <div className='col-sm-12 col-md-2 col-lg-2 classroomTimetablesRow-cell'>\r\n                                <p> {moment(val.start, 'HH:mm:ss').format('HH:mm')} - {moment(val.end, 'HH:mm:ss').format('HH:mm')} </p>\r\n                                <p> {val.teacher_name} </p>\r\n                                <p> {val.subject_title} </p>\r\n                                <div className=\"d-flex w-100 justify-content-between\">\r\n                                    <h5 className=\"mb-1\" onClick={() => { index = i; setShow(!show) }}> <BsFillGearFill /> </h5>\r\n                                    <small className={val.capacity > val.maxCapacity ? 'red' : 'green'}>\r\n                                        {val.capacity} / {val.maxCapacity}\r\n                                    </small>\r\n                                </div>\r\n                            </div>\r\n                        </>\r\n                    )\r\n                })}\r\n                {show ? <ClassroomTimetablesRowUpdate data={data[index]} key={'ClassroomTimetablesRowUpdate0'} /> : \"\"}\r\n            </div>\r\n        </>\r\n    )\r\n}\r\n\r\nconst ClassroomTimetablesRowUpdate = ({ data }) => {\r\n    const [showUpdate, setShow] = useState(0);\r\n\r\n    return (\r\n        <>\r\n            <div className='new-line'></div>\r\n            <div className='col-sm-12 col-md-12 col-lg-12 classroomTimetablesRowUpdate-container'>\r\n                <h5>\r\n                    Více informací o použití učebny v {moment(data.date).format('DD.MM.YYYY')} od {moment(data.start, 'HH:mm:ss').format('HH:mm')} do {moment(data.end, 'HH:mm:ss').format('HH:mm')}\r\n                </h5>\r\n                <div className='row classroomTimetablesRowUpdate-div'>\r\n                    <div className='col-sm-1 col-md-1 col-lg-1'>\r\n                        <Tooltip title=\"Zobrazit studenty\">\r\n                            <IconButton onClick={() => { setShow(0) }}>\r\n                                <FaUserAlt />\r\n                            </IconButton>\r\n                        </Tooltip>\r\n                    </div>\r\n                    <div className='col-sm-1 col-md-1 col-lg-1'>\r\n                        <Tooltip title=\"Upravit záznam učebny\">\r\n                            <IconButton onClick={() => { setShow(1) }}>\r\n                                <BsPencilFill />\r\n                            </IconButton>\r\n                        </Toolt