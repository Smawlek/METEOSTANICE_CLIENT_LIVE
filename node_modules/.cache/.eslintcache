] = useState(0);\n    const [active, setActive] = useState(1);\n    const [name, setName] = useState(0);\n\n    // Pro obsah Selectů\n    const [teacherOptions, setTeacherOptions] = useState([]);\n    const [locationOptions, setLocationOptions] = useState([{ label: 'Nezáleží', value: 0 }]);\n    const [locationOptionsMinus, setLocationOptionsMinus] = useState([]);\n\n    // Ostatní\n    const [loading, setLoading] = useState(false);\n\n    const [result, setResult] = useState(\"\");\n\n    useEffect(() => {\n        if (!callOnce) {\n            callOnce = true;\n            getNames();\n            getSubjects();\n            getLocations();\n            listAll();\n        }\n    }, [])\n\n    async function getSubjects() {\n        let response = await _listSubjects({ token: token });\n\n        setSubjectSelect(response.data);\n    }\n\n    async function getLocations() {\n        let arr = [{ label: 'Nezáleží', value: 0 }];\n        let arr2 = [];\n        let data = (await _listLocations({ token: token })).data;\n\n        for (let i = 0; i < data.length; i++) {\n            arr.push({\n                value: data[i].id_lo,\n                label: data[i].location\n            });\n            arr2.push({\n                value: data[i].id_lo,\n                label: data[i].location\n            });\n        }\n\n        setLocationOptions(arr);\n        setLocationOptionsMinus(arr2);\n    }\n\n    async function getNames() {\n        if (!getNamesOnce) {\n            return;\n        }\n\n        getNamesOnce = false;\n\n        setNames((await _getTeacherNames({ token: token })).data);\n    }\n\n    function setNames(names) {\n        let temp = [{ value: 0, label: \"Nezáleží\" }];\n\n        for (let i = 0; i < names.length; i++) {\n            temp.push({\n                value: names[i].name,\n                label: names[i].name\n            });\n        }\n\n        setTeacherOptions(temp);\n    }\n\n    function setSubjectSelect(subject) {\n        subjects = [];\n\n        for (let i = 0; i < subject.length; i++) {\n            subjects.push({\n                value: subject[i].id_su,\n                label: subject[i].title\n            });\n        }\n    }\n\n    function handleSubjectOptions(selected) {\n        setSubject(selected.value);\n    }\n\n    async function search() {\n        setListAllBtn('');\n        setResult([]);\n\n        if (name != 0) {\n            searchByName();\n            return;\n        }\n\n        try {\n            let response = await _getFilteredTeachers({\n                subject: Number(subject),\n                location: Number(where),\n                gender: Number(gender),\n                active: Number(active),\n                token: token\n            });\n\n            x = 0;\n\n            filter(response.data);\n        } catch (error) {\n            throw new Error(error);\n        }\n    }\n\n    async function searchByName() {\n        try {\n            console.log(name)\n            let response = await _getTeacherByName({\n                name: name,\n                token: token\n            });\n\n            x = 0;\n\n            filter(response.data);\n        } catch (error) {\n            throw new Error(error);\n        }\n    }\n\n    async function listAll() {\n        setListAllBtn('disabled');\n        setLoading(true);\n\n        try {\n            let response = await _getTeachers({ token: token });\n\n            x = 0;\n\n            filter(response.data);\n            setLoading(false);\n        } catch (error) {\n            throw new Error(error);\n        }\n    }\n\n    async function listTeachersWithKeys() {\n        setListAllBtn('');\n\n        try {\n            let response = await _getTeachersWithKeys()\n\n            x = 0;\n\n            filter(response.data);\n        } catch (error) {\n            throw new Error(error);\n        }\n    }\n\n    function filter(data) {\n        let temp = [];\n        let names = [];\n\n        for (let i = 0; i < data.length; i++) {\n            if (!names.includes(data[i].name)) {\n                names.push(data[i].name)\n                temp.push(data[i]);\n            }\n        }\n\n        setResult(temp)\n    }\n\n    return (\n        <>\n            <div>\n                <h3> Vyhledávání lektorů: </h3>\n\n                <div className='row'>\n                    <div className=\"col-sm-12 col-md-12 col-lg-11 mx-auto\">\n                        <label> Jméno lektora\n                            <Tooltip placement='right' title=\"Po vybrání jména lektora budou všechny ostatní filtry ignorovány\">\n                                <IconButton>\n                                    <AiOutlineInfoCircle />\n                                </IconButton>\n                            </Tooltip>\n                        </label>\n                        <Select\n                            aria-labelledby=\"aria-label\"\n                            inputId=\"aria-example-input\"\n                            name=\"aria-live-color\"\n                            placeholder=\"Vyberte jméno lektora..\"\n                            defaultValue={{ value: 0, label: \"Nezáleží\" }}\n                            onChange={(selected) => { setName(selected.value) }}\n                            options={teacherOptions}\n                        />\n                    </div>\n                </div>\n\n                <div className='new-line'></div>\n                <div className='row'>\n                    <div className=\"col-sm-12 col-md-12 col-lg-5 mx-auto margin-bottom\">\n                        <label> Předmět </label>\n                        <Select\n                            aria-labelledby=\"aria-label\"\n                            inputId=\"aria-example-input\"\n                            name=\"aria-live-color\"\n                            placeholder=\"Vyberte jednotku..\"\n                            defaultValue={{ value: 1, label: \"Matematika\" }}\n                            onChange={handleSubjectOptions}\n                            options={subjects}\n                        />\n                    </div>\n                    <div className=\"col-sm-12 col-md-12 col-lg-5 mx-auto\">\n                        <label> Lokace </label>\n                        <Select\n                            aria-labelledby=\"aria-label\"\n                            inputId=\"aria-example-input\"\n                            name=\"aria-live-color\"\n                            placeholder=\"Vyberte lokaci..\"\n                            defaultValue={{ label: 'Nezáleží', value: 0 }}\n                            onChange={(e) => { setWhere(e.value) }}\n                            options={locationOptions}\n                        />\n                    </div>\n                </div>\n\n                <div className='new-line'></div>\n                <div className='row'>\n                    <div className=\"col-sm-12 col-md-12 col-lg-5 mx-auto margin-bottom\">\n                        <label> Pohlaví </label>\n                        <select className=\"form-control\" onChange={(e) => setGender(e.target.value)}>\n                            <option value={0} defaultValue> Nezáleží </option>\n                            <option value={1}> Muž </option>\n                            <option value={2}> Žena </option>\n                        </select>\n                    </div>\n                    <div className=\"col-sm-12 col-md-12 col-lg-5 mx-auto\">\n                        <label> Aktivní </label>\n                        <Select\n                            aria-labelledby=\"aria-label\"\n                            inputId=\"aria-example-input\"\n                            name=\"aria-live-color\"\n                            placeholder=\"Vyberte jestli je lektor aktivní\"\n                            defaultValue={{ value: 1, label: \"Aktivní\" }}\n                            onChange={(selected) => { setActive(selected.value) }}\n                            options={activeOptions}\n                        />\n                    </div>\n                </div>\n\n                <div className='new-line'></div>\n                <div className='row'>\n                    <div className=\"col-sm-12 col-md-12 col-lg-4 mx-auto margin-bottom\">\n                        <button\n                            type=\"button\"\n                            className=\"btn btn-success btn-lg col-sm-12 col-md-12 col-lg-12\"\n                            disabled={listAllBtn}\n                            onClick={() => { listAll() }}>\n                            Zobrazit všechny lektory\n                            {loading ? \" - data se načítají\" : \"\"}\n                        </button>\n                    </div>\n                    <div className=\"col-sm-12 col-md-12 col-lg-4 mx-auto margin-bottom\">\n                        <button\n                            type=\"button\"\n                            className=\"btn btn-success btn-lg col-sm-12 col-md-12 col-lg-12\"\n                            onClick={() => { listTeachersWithKeys() }}>\n                            Zobrazit lektory co mají klíče\n                        </button>\n                    </div>\n                    <div className=\"col-sm-12 col-md-12 col-lg-4 mx-auto\">\n                        <button\n                            type=\"button\"\n                            className=\"btn btn-success btn-lg col-sm-12 col-md-12 col-lg-12\"\n                            onClick={() => { search() }}>\n                            Vyhledat\n                        </button>\n                    </div>\n                </div>\n            </div>\n\n            <div className='line-divider'></div>\n            {\n                result == \"\" ?\n                    <div className=\"col-sm-12 col-md-12 col-lg-12 mx-auto search-result\"> <p> <b> ŽÁDNÉ VÝSLEDKY </b> </p> </div>\n                    : result.map((val, i) => {\n                        if (x < result.length) {\n                            return (\n                                <TeacherSearchCard key={i} data={result[i]} subjects={subjects} locationOptions={locationOptionsMinus} />\n                            );\n                        }\n                    })\n            }\n\n\n            <Outlet />\n        </>\n    )\n};\n\nconst TeacherSearchCard = (props) => {\n\n    const [show, setShow] = useState(0);\n    // Data\n    const [name, setName] = useState(props.data.name);\n    const [hasKeys, setHasKeys] = useState(props.data.hasKeys);\n    const [phone, setPhone] = useState(props.data.phone);\n    const [email, setEmail] = useState(props.data.email);\n    const [active, setActive] = useState(props.data.active);\n    const [gender, setGender] = useState(props.data.gender);\n\n    useEffect(() => {\n    }, [])\n\n    useEffect(() => {\n        setName(props.data.name)\n        setHasKeys(props.data.hasKeys);\n        setPhone(props.data.phone);\n        setEmail(props.data.email);\n        setActive(props.data.active);\n        setGender(props.data.gender);\n    }, [props])\n\n    function hide() {\n        setShow(0);\n    }\n\n    function updateData(data) {\n        setName(data.name)\n        setHasKeys(data.hasKeys);\n        setPhone(data.phone);\n        setEmail(data.email);\n        setActive(data.active);\n        setGender(data.gender);\n    }\n\n    return (\n        <div className='row margin-bottom'>\n            <div>\n                <div className=\"card col-sm-12 col-md-12 col-lg-12 mx-auto\">\n                    <div className=\"card-body\">\n                        <h5 className=\"card-title\"> {name}\n                            <Button\n                                className='teacherSearch-activityBtn'\n                            >\n                                <span className={active == 1 ? \"teacherSearch-activityIcon green\" : active == 0 ? \"teacherSearch-activityIcon red\" : \"teacherSearch-activityIcon yellow\"}>\n                                    {active == 1 ? <TiTick /> : active == 0 ? <ImCross /> : <BsFillPauseFill />}\n                                </span>\n                            </Button>\n                            <BsFillKeyFill className={hasKeys ? 'mb-1 green' : 'hidden'} size={25} />\n                        </h5>\n                        <h6 className=\"card-subtitle mb-2 text-muted\"> {gender === 1 ? \"Muž\" : gender === 2 ? \"Žena\" : gender} </h6>\n\n                        <p className=\"card-text\"> <AiFillPhone /> {phone == \"\" || phone == null || phone == 0 ? \" Lektor nemá vyplněné telefonní číslo\" : \" \" + phone} </p>\n                        <p className=\"card-text\"> <MdAlternateEmail /> {email == \"\" || email == null ? \" Lektor nemá vyplněný email\" : \" \" + email} </p>\n                        <div>\n                            <Tooltip title={show === 1 ? \"Schovat úpravu lektora\" : \"Upravit lektora\"}>\n                                <IconButton onClick={() => { show === 1 ? setShow(0) : setShow(1) }}>\n                                    <BsFillGearFill />\n                                </IconButton>\n                            </Tooltip>\n                            <Tooltip title={show === 5 ? \"Schovat úpravu časových možností\" : \"Zobrazit úpravu časových možností\"}>\n                                <IconButton onClick={() => { show === 5 ? setShow(0) : setShow(5) }}>\n                                    <FaHourglassHalf />\n                                </IconButton>\n                            </Tooltip>\n                            <Tooltip title={show === 2 ? \"Schovat více informací\" : \"Zobrazit více informací\"}>\n                                <IconButton onClick={() => { show === 2 ? setShow(0) : setShow(2) }}>\n                                    <BsInfoCircleFill />\n                                </IconButton>\n                            </Tooltip>\n                            <Tooltip title={show === 3 ? \"Schovat přeměty\" : \"Zobrazit předměty\"}>\n                                <IconButton onClick={() => { show === 3 ? setShow(0) : setShow(3) }}>\n                                    <MdSchool />\n                                </IconButton>\n                            </Tooltip>\n                            <Tooltip title={show === 4 ? \"Schovat smlouvy a růžová prohlášení\" : \"Zobrazit smlouvy a růžová prohlášení\"}>\n                                <IconButton onClick={() => { show === 4 ? setShow(0) : setShow(4) }}>\n                                    <RiFilePaper2Fill className={moment().isAfter(props.data.contract_end) && props.data.contract_end != null ? \"red\" : \"\"} />\n                                </IconButton>\n                            </Tooltip>\n                        </div>\n                        {show === 1 ? <TeacherSearchEdit data={props.data} updateData={updateData} locationOptions={props.locationOptions} hide={hide} key={1} /> : \"\"}\n                        {show === 2 ? <TeacherSearchMoreInfo data={props.data} key={2} /> : \"\"}\n                        {show === 3 ? <TeacherSearchSubjects data={props.data} hide={hide} subjects={subjects} key={3} /> : \"\"}\n                        {show === 4 ? <TeacherSearchContracts data={props.data} hide={hide} key={4} /> : \"\"}\n                        {show === 5 ? <TeacherSearchUpdateTimeposibilities data={props.data} hide={hide} /> : \"\"}\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nconst TeacherSearchMoreInfo = (props) => {\n    return (\n        <>\n            <div className='line-divider'></div>\n            <div>\n                <h6> Více informací </h6>\n            </div>\n\n            <div className='new-line'></div>\n            <div className='row'>\n                <div className='col-sm-12 col-md-12 col-lg-6'>\n                    <ul className=\"list-group\">\n                        <li className=\"list-group-item\"> Pohlaví: {props.data.gender} </li>\n                        <li className=\"list-group-item\"> Hodin týdně: {props.data.hoursPerWeek} </li>\n                        {/*<li className=\"list-group-item\"> Počet odučených lekcí: {props.data.lesson_number} </li>*/}\n                        <li className=\"list-group-item\"> Plat pro ZŠ Online: {props.data.zs1On} </li>\n                        <li className=\"list-group-item\"> Plat pro SŠ Online: {props.data.ssOn} </li>\n                        <li className=\"list-group-item\"> Plat pro VŠ Online: {props.data.vsOn} </li>\n                        <li className=\"list-group-item\"> Číslo bankovního účtu: {props.data.bankNumber} </li>\n                    </ul>\n                </div>\n\n                <div className='col-sm-12 col-md-12 col-lg-6'>\n                    <ul className=\"list-group\">\n                        <li className=\"list-group-item\"> Datum přidání: {moment(props.data.arrival).format('YYYY-MM-DD') == \"1899-11-30\" ? \"Datum nebylo vyplněno\" : moment(props.data.arrival).format('MM/YYYY')} </li>\n                        <li className=\"list-group-item\"> Datum narození: {moment(props.data.birthday).format('DD.MM.YYYY')} </li>\n                        <li className=\"list-group-item\"> Plat pro ZŠ Offline: {props.data.zs1Off} </li>\n                        <li className=\"list-group-item\"> Plat pro SŠ Offline: {props.data.ssOff} </li>\n                        <li className=\"list-group-item\"> Plat pro VŠ Offline: {props.data.vsOff} </li>\n                    </ul>\n                </div>\n\n                <div className='new-line'></div>\n                <div className='col-sm-12 col-md-12 col-lg-12'>\n                    <ul className=\"list-group\">\n                        <li className=\"list-group-item\"> Časové možnosti: {props.data.timePossibilities} </li>\n                        <li className=\"list-group-item\"> Poznámka: {props.data.notes} </li>\n                    </ul>\n                </div>\n            </div>\n            <Outlet />\n        </>\n    )\n};\n\nconst TeacherSearchUpdateTimeposibilities = ({ data, hide }) => {\n    // Časové možnosti\n    const [timeposibilitiesPo, setTimeposibilitiesPo] = useState(\"\");\n    const [timeposibilitiesUt, setTimeposibilitiesUt] = useState(\"\");\n    const [timeposibilitiesSt, setTimeposibilitiesSt] = useState(\"\");\n    const [timeposibilitiesCt, setTimeposibilitiesCt] = useState(\"\");\n    const [timeposibilitiesPa, setTimeposibilitiesPa] = useState(\"\");\n    const [timeposibilitiesSo, setTimeposibilitiesSo] = useState(\"\");\n    const [timeposibilitiesNe, setTimeposibilitiesNe] = useState(\"\");\n    // Proměnné k odeslání\n    const [errMsg, setErrMsg] = useState('');\n    // Errory\n    const [timeposibilitiesErr, setTimeposibilitiesErr] = useState(false);\n    const [poErr, setPoErr] = useState(false);\n    const [utErr, setUtErr] = useState(false);\n    const [stErr, setStErr] = useState(false);\n    const [ctErr, setCtErr] = useState(false);\n    const [paErr, setPaErr] = useState(false);\n    const [soErr, setSoErr] = useState(false);\n    const [neErr, setNeErr] = useState(false);\n\n    useEffect(() => {\n        //prepareTimeposibilities();\n    }, [])\n\n    useEffect(() => {\n        if (timeposibilitiesPo === \"\" || timeposibilitiesUt === \"\" || timeposibilitiesSt === \"\" || timeposibilitiesCt === \"\" || timeposibilitiesPa === \"\"\n            || timeposibilitiesSo === \"\" || timeposibilitiesNe === \"\") {\n            return\n        }\n\n        setTimeposibilitiesErr(false);\n    }, [timeposibilitiesPo, timeposibilitiesUt, timeposibilitiesSt, timeposibilitiesCt, timeposibilitiesPa, timeposibilitiesSo, timeposibilitiesNe])\n\n    useEffect(() => {\n        setPoErr(false);\n    }, [timeposibilitiesPo])\n\n    useEffect(() => {\n        setUtErr(false);\n    }, [timeposibilitiesUt])\n\n    useEffect(() => {\n        setStErr(false);\n    }, [timeposibilitiesSt])\n\n    useEffect(() => {\n        setCtErr(false);\n    }, [timeposibilitiesCt])\n\n    useEffect(() => {\n        setPaErr(false);\n    }, [timeposibilitiesPa])\n\n    useEffect(() => {\n        setSoErr(false);\n    }, [timeposibilitiesSo])\n\n    useEffect(() => {\n        setNeErr(false);\n    }, [timeposibilitiesNe])\n\n    function prepareTimeposibilities() {\n        let temp = (data.timePossibilities).split(':');\n\n        setTimeposibilitiesPo(((temp[1]).split(\", \"))[0]);\n        setTimeposibilitiesUt(((temp[2]).split(\", \"))[0]);\n        setTimeposibilitiesSt(((temp[3]).split(\", \"))[0]);\n        setTimeposibilitiesCt(((temp[4]).split(\", \"))[0]);\n        setTimeposibilitiesPa(((temp[5]).split(\", \"))[0]);\n        setTimeposibilitiesSo(((temp[6]).split(\", \"))[0]);\n        setTimeposibilitiesNe(temp[7]);\n    }\n\n    function check() {\n        let containsError = false;\n\n        if (timeposibilitiesPo === \"\" || timeposibilitiesUt === \"\" || timeposibilitiesSt === \"\" || timeposibilitiesCt === \"\" || timeposibilitiesPa === \"\" ||\n            timeposibilitiesSo === \"\" || timeposibilitiesNe === \"\") {\n            containsError = true;\n            setPoErr(timeposibilitiesPo === \"\" ? true : false);\n            setUtErr(timeposibilitiesUt === \"\" ? true : false);\n            setStErr(timeposibilitiesSt === \"\" ? true : false);\n            setCtErr(timeposibilitiesCt === \"\" ? true : false);\n            setPaErr(timeposibilitiesPa === \"\" ? true : false);\n            setSoErr(timeposibilitiesSo === \"\" ? true : false);\n            setNeErr(timeposibilitiesNe === \"\" ? true : false);\n            setTimeposibilitiesErr(true);\n        }\n\n        if (containsError) {\n            setErrMsg(\"* Nebyli vyplněny všechny dny\");\n            return;\n        }\n\n        send();\n    }\n\n    async function send() {\n        const dat = {\n            timeposibilities: \"PO: \" + timeposibilitiesPo + \", ÚT: \" + timeposibilitiesUt + \", ST: \" + timeposibilitiesSt + \", ČT: \" + timeposibilitiesCt + \", PÁ: \" +\n                timeposibilitiesPa + \", SO: \" + timeposibilitiesSo + \", NE: \" + timeposibilitiesNe,\n        }\n\n        if (await _updateTeachersTimeposibilities(dat)) {\n            alert(\"Časové možnosti lektora byli aktualizovány\");\n            hide();\n        }\n    }\n\n    return (\n        <>\n            <div className='line-divider'></div>\n            <div>\n                <h6> Úprava časových možností lektora </h6>\n                <a> Předchozí: {data.timePossibilities == \"\" ? \"Nebylo vyplněno\" : data.timePossibilities} </a>\n            </div>\n\n            <div className='new-line'></div>\n            <div className='row'>\n                <div className='col-sm-12 col-md-3 col-lg-3 mx-auto margin-bottom'>\n                    <TextField\n                        error={poErr}\n                        label=\"Pondělí*\"\n                        type=\"text\"\n                        defaultValue=\"\"\n                        value={timeposibilitiesPo}\n                        onChange={(e) => { setTimeposibilitiesPo(e.target.value) }}\n                    />\n                </div>\n                <div className='col-sm-12 col-md-3 col-lg-3 mx-auto margin-bottom'>\n                    <TextField\n                        error={utErr}\n                        label=\"Úterý*\"\n                        type=\"text\"\n                        defaultValue=\"\"\n                        value={timeposibilitiesUt}\n                        onChange={(e) => { setTimeposibilitiesUt(e.target.value) }}\n                    />\n                </div>\n                <div className='col-sm-12 col-md-3 col-lg-3 mx-auto margin-bottom'>\n                    <TextField\n                        error={stErr}\n                        label=\"Středa*\"\n                        type=\"text\"\n           