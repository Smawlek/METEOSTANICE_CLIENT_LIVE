s />}></Route>\n              <Route path=\"obchodni-podminky\" element={<Terms />}></Route>\n              <Route path=\"cenik\" element={<Prices />}></Route>\n              <Route path=\"kontakt\" element={<Contact />}></Route>\n              {/* Stránky pro typy škol */}\n              <Route path=\"zs\" element={<ZS />}></Route>\n              <Route path=\"ss\" element={<SS />}></Route>\n              <Route path=\"vs\" element={<VS />}></Route>\n              {/* Přihlášení */}\n              <Route path=\"log\" element={<Log />}></Route>\n              {/* Stránky pro přihlášené */}\n              <Route path=\"timetable\" element={<TimetableSite />}></Route>\n              <Route path=\"profile\" element={<ProfileSite />}></Route>\n              <Route path=\"teacher-search\" element={<TeacherSearchSite />}></Route>\n              <Route path=\"contact-list\" element={<ContactListSite />}></Route>\n              <Route path=\"lesson-create\" element={<CreateLessonSite />}></Route>\n              <Route path=\"client-list\" element={<ClientListSite />}></Route>\n              <Route path=\"invoice-create\" element={<InvoiceCreateSite />}></Route>\n              <Route path=\"invoice-list\" element={<InvoiceListSite />}></Route>\n              <Route path=\"teacher-create\" element={<TeacherCreateSite />}></Route>\n              <Route path=\"client-create\" element={<ClientCreateSite />}></Route>\n              <Route path=\"lesson-search\" element={<LessonSearchSite />}></Route>\n              <Route path=\"salary-calculation\" element={<SalaryCalculationSite />}></Route>\n              <Route path=\"contract-search\" element={<ContractSearchSite />}></Route>\n              <Route path=\"other\" element={<Other />}></Route>\n              <Route path=\"checks\" element={<ChecksSite />}></Route>\n              {/* Pravidelné hodiny */}\n              <Route path=\"regular-lessons\" element={<RegularLessonsList />}></Route>\n              <Route path=\"regular