la, setLA] = useState(false);\n    const [ch, setCH] = useState(false);\n    const [de, setDE] = useState(false);\n    const [other, setOther] = useState(false);\n    const [cj, setCJ] = useState(false);\n    const [nj, setNJ] = useState(false);\n    const [sj, setSJ] = useState(false);\n    const [cpc, setCPC] = useState(false);\n    const [fy, setFY] = useState(false);\n    const [bi, setBI] = useState(false);\n    const [zsv, setZSV] = useState(false);\n\n    // Pro práci s chybami\n    const [errMsg, setErr] = useState(\"\");\n    const [nameErr, setNameErr] = useState(false);\n    const [emailErr, setEmailErr] = useState(false);\n    const [lessonErr, setLessonErr] = useState(false);\n    const [achieveErr, setAchieveErr] = useState(false);\n    const [howManyErr, setHowManyErr] = useState(false);\n    const [agreeErr, setAgreeErr] = useState(false);\n    const [success, setSuccess] = useState(false);\n\n    useEffect(() => {\n        setErr(\"\");\n    }, [name, email, lessonChosen, type, where, achieve, howMany])\n\n    useEffect(() => {\n        if (ma || aj || fj || rj || la || ch || de || other || cj || nj || sj || cpc || fy || bi || zsv) {\n            firstLesson = false;\n            setLessonErr(false);\n            setLessonChosen(true);\n        } else {\n            setLessonErr(true);\n            setLessonChosen(false);\n        }\n    }, [ma, aj, fj, rj, la, ch, de, other, cj, nj, sj, cpc, fy, bi, zsv])\n\n    useEffect(() => {\n        setNameErr(false);\n    }, [name])\n\n    useEffect(() => {\n        setEmailErr(false);\n    }, [email])\n\n    useEffect(() => {\n        setAchieveErr(false);\n    }, [achieve])\n\n    useEffect(() => {\n        setHowManyErr(false);\n    }, [howMany])\n\n    useEffect(() => {\n        setAgreeErr(false);\n    }, [agree])\n\n    function assign(target) {\n        switch (target) {\n            case \"1\":\n                setName(\"\");\n                break;\n            case \"2\":\n                setEmail(